{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "ignoreDeprecations": "5.0",
        "sourceMap": true,
        "strict": true,
        "rootDirs": [
            ".",
            "./.svelte-kit/types"
        ]
    }
}